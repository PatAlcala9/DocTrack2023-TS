    "core-js": "^3.33.0",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.4",
    "gsap": "^3.12.2",
    "html-to-image": "^1.11.11",
    "jspdf": "^2.5.1",
    "md5": "^2.3.0",
    "pdf-lib": "^1.17.1",
    "pinia": "^2.1.6",
